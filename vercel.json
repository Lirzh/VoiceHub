{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".output",
  "framework": "nuxt",
  "functions": {
    "api/**/*.js": {
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 